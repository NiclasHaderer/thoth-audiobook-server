{
  "ignoreFile": ".thothignore",
  "production": false,
  "allowNewSignups": true,
  "fullScanCron": "0 0 * * 1",
  "audioFileLocations": [
    "test-resources"
  ],
  "port": 8080,
  "domain": "localhost",
  "TLS": false,
  "jwtCertificate": "config-preset/jwt.pem",
  "database": {
    "driverClassName": "org.sqlite.JDBC",
    "jdbcUrl": "jdbc:sqlite:thoth.db",
    "maximumPoolSize": 1,
    "autoCommit": false,
    "transactionIsolation": "TRANSACTION_SERIALIZABLE"
  }
}