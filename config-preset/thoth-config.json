{
  "ignoreFile": ".thothignore",
  "production": false,
  "fullScanCron": "0 0 * * 1",
  "metadataRefreshCron": "0 0 * * 1",
  "audioFileLocations": [
    "test-resources"
  ],
  "analyzerThreads": 6,
  "port": 8080,
  "domain": "localhost",
  "TLS": false,
  "audibleRegion": "us",
  "configDirectory": "config-preset",
  "preferEmbeddedMetadata": true,
  "database": {
    "driverClassName": "org.h2.Driver",
    "jdbcUrl": "jdbc:h2:mem:regular",
    "maximumPoolSize": 100,
    "autoCommit": false,
    "transactionIsolation": "TRANSACTION_REPEATABLE_READ"
  }
}